{
    "version": 2,
    "builds": [
      {
        "src": "Backend/src/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "Frontend/index.html",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "Frontend/dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "Backend/src/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "Frontend/dist/$1"
      }
    ]
  }
  